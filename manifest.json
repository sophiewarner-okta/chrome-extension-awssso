{
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.3.3",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "https://*.console.aws.amazon.com/*",
    "https://*.amazon.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.awsapps.com/start#/",
        "https://*.awsapps.com/start/*",
        "https://*.console.aws.amazon.com/*",
        "https://health.aws.amazon.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/fav.png"
      ],
      "matches": [
        "https://*.awsapps.com/*",
        "https://*.console.aws.amazon.com/*",
        "https://health.aws.amazon.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html"
  }
}